{
  "name": "contracts",
  "version": "1.0.0",
  "description": "Magic Mondrian NFT launch contracts",
  "author": "Whitelabel Solutions",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whitelabel-market/erc721.git"
  },
  "keywords": [
    "erc721",
    "erc721A",
    "smart",
    "contract",
    "solidity",
    "hardhat"
  ],
  "scripts": {
    "prepare": "husky install",
    "rename-contract": "hardhat rename-contract",
    "configure-contract": "hardhat configure-contract",
    "generate-voucher": "hardhat generate-voucher",
    "lint": "npx solium -d contracts",
    "lint:fix": "npx solium -d contracts --fix",
    "hh-node": "hardhat node",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test-coverage": "hardhat coverage",
    "test-and-coverage": "hardhat test && hardhat coverage",
    "deploy-testnet": "hardhat run --network mumbai scripts/1_deploy.ts",
    "deploy-mainnet": "hardhat run --network mainnet scripts/1_deploy.ts",
    "deploy-local": "hardhat run --network localhost scripts/1_deploy.ts",
    "deploy-splitter": "hardhat run --network mumbai scripts/7_deploy_splitter.ts",
    "verify-testnet": "hardhat verify --constructor-args config/ContractArguments.ts --network mumbai",
    "verify-mainnet": "hardhat verify --constructor-args config/ContractArguments.ts --network mainnet",
    "whitelist-open": "hardhat run scripts/2_whitelist_open.ts",
    "publicsale-open": "hardhat run scripts/3_public_sale_open.ts",
    "pause": "hardhat run scripts/4_pause.ts",
    "unpause": "hardhat run scripts/5_unpause.ts",
    "reveal": "hardhat run scripts/6_reveal.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.9.1",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.4",
    "keccak256": "^1.0.6",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2",
    "typescript": "^4.5.2",
    "vue-tsc": "^0.34.10"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0"
  }
}
